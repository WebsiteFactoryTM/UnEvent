{
  "name": "@unevent/backend",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation PORT=4000 next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation PORT=4000 next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload ",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start -p $PORT",
    "seed": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=4096\" payload run seed",
    "migrate:custom": "node scripts/migrate.mjs",
    "migrate:create": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:create",
    "migrate:status": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:status",
    "generate:cities": "node scripts/generate-cities.mjs",
    "migrate:run": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
    "copy:types": "node scripts/copy-types-to-frontend.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.933.0",
    "@aws-sdk/s3-request-presigner": "^3.933.0",
    "@payloadcms/db-postgres": "3.59.1",
    "@payloadcms/email-resend": "^3.65.0",
    "@payloadcms/next": "3.59.1",
    "@payloadcms/payload-cloud": "3.59.1",
    "@payloadcms/plugin-cloud-storage": "^3.64.0",
    "@payloadcms/plugin-search": "3.59.1",
    "@payloadcms/plugin-sentry": "^3.65.0",
    "@payloadcms/richtext-lexical": "3.59.1",
    "@payloadcms/ui": "3.59.1",
    "@react-email/components": "^0.0.20",
    "@react-email/render": "^1.1.2",
    "@sentry/nextjs": "^10",
    "@types/express": "^5.0.3",
    "@types/pg": "^8.15.6",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@unevent/shared": "workspace:*",
    "@upstash/redis": "^1.35.6",
    "bullmq": "^5",
    "cross-env": "^7.0.3",
    "csv-parse": "^6.1.0",
    "dotenv": "16.4.7",
    "graphql": "^16.8.1",
    "ioredis": "^5.8.2",
    "lunr": "^2.3.9",
    "next": "15.4.4",
    "node-cron": "^4.2.1",
    "payload": "3.59.1",
    "payload-oapi": "^0.2.5",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-email": "^3.0.1",
    "sharp": "0.34.2",
    "slugify": "^1.6.6",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "1.54.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/node-cron": "^3.0.11",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.4",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "prettier": "^3.4.2",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  }
}
