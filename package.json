{
  "name": "unevent",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=20.11.1 <21"
  },
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:infra": "docker compose up -d",
    "down:infra": "docker compose down",
    "ps": "docker compose ps",
    "dev:backend": "pnpm --filter @unevent/backend dev",
    "build:backend": "pnpm --filter @unevent/backend build",
    "start:backend": "pnpm --filter @unevent/backend start",
    "generate:types": "pnpm --filter @unevent/backend generate:types"
  }
}
